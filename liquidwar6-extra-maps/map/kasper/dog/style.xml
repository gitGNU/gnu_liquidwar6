<?xml version="1.0"?>
<lw6mapparam>


<float key="cursor-size" value="0.8" />


	<!--
		turn this one on, if you want to define your own color values ... a lot
		of stuff would also need to be turned on in hints.xml
	-->
	<bool key="colorize" value="true" />


	<!-- 
		A lot of colors will be grabbed from these four, if color-auto is enabled
		and various guess-colors is disabled in hints.xml
	-->
	<color key="color-base-bg" value="#1e3200" /> <!-- black part of sky -  -->
	<color key="color-base-fg" value="#8aa06b" /> <!-- white part of sky -  -->
	<color key="color-alternate-bg" value="#1e3200" /> <!-- GUI bg color - HUD text shadow -->
	<color key="color-alternate-fg" value="#8aa06b" /> <!-- GUI text color - Bubbles - cursor color -->







</lw6mapparam>