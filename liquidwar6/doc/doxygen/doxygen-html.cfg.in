@PROJECT_NAME = @PACKAGE_TARNAME@
@PROJECT_NUMBER = @VERSION@
@PROJECT_BRIEF = @PACKAGE_STRING@
@INPUT = $(DOXYGEN_INPUT)
@RECURSIVE = YES
@FILE_PATTERNS = *.h *.h.in *.c
@GENERATE_XML = NO
@GENERATE_HTML = YES
@GENERATE_LATEX = NO
@OPTIMIZE_OUTPUT_FOR_C = YES
@HAVE_DOT = @DOXYGEN_HAVE_DOT@
@DOT_IMAGE_FORMAT = svg
@INLINE_SOURCES = YES
@SOURCE_BROWSER  = YES
@FULL_PATH_NAMES = YES
@STRIP_FROM_PATH = @DOXYGEN_SRCDIR@
@REFERENCES_RELATION = YES
@REFERENCED_BY_RELATION = YES
@ALPHABETICAL_INDEX = YES
@CALL_GRAPH = YES
@CALLER_GRAPH = YES
@DIRECTORY_GRAPH = YES
@EXTRACT_ALL = YES