	cd ${PACKAGE_TMP} && unzip ${DLL_ZIP}
	cd ${TMP} && zip -r @PACKAGE_TARNAME@-@PACKAGE_VERSION@-ms-windows.zip @PACKAGE_TARNAME@-@PACKAGE_VERSION@
	mv ${TMP}/@PACKAGE_TARNAME@-@PACKAGE_VERSION@-ms-windows.zip .
