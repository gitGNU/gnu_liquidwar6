	${top_srcdir}/src/utod.sh `find ${PACKAGE_TMP} -name "*.txt"`
	${top_srcdir}/src/utod.sh `find ${PACKAGE_TMP} -name "*.html"`
	cp ../doc/liquidwar6.pdf ${PACKAGE_TMP}/doc/
