<?xml version="1.0"?>
<lw6mapparam>

<float key="wall-grease" value="-4"/>


  <float key="speed" value="0.6" />


    <!-- If true, the colors below will be picked from texture.jpeg
		color-base-bg
		color-base-fg
		color-alternate-bg
		color-alternate-fg
-->
 <bool key="guess-colors" value="false" />  
  
  
  
  <!-- all of these "color-auto" assigns colors automatically, from the following:
	  color-base-bg
	  color-base-fg
	  color-alternate-bg
	  color-alternate-fg
  -->

  <bool key="background-color-auto" value="true" />
  <bool key="hud-color-auto" value="true" />
  <bool key="menu-color-auto" value="true" />
  <bool key="view-color-auto" value="true" />


</lw6mapparam>
