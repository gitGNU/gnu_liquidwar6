#ifndef LIQUIDWAR6LDR_PNG_H
#define LIQUIDWAR6LDR_PNG_H

/*
 * libpng has its version present in the header path
 * for instance libpng12/png.h or libpng14/png.h so
 * we need to have this path dynamically configured
 * depending on what ./configure detected. Else one
 * can end using the wrong png library, for instance 1.2
 * when some other library (typically SDL) is linked
 * against 1.4. See bug 30354:
 * https://savannah.gnu.org/bugs/?30354
 */
#include <@LIBPNG_1X_H@>

#endif
